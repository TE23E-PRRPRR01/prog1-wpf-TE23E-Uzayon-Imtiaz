<Window x:Class="ASDASDASD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ASDASDASD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinHeight="200" MinWidth="300" SizeToContent="WidthAndHeight">
    <StackPanel>
        <Label FontSize="32" FontWeight="Bold" Padding="5">Portoräknare</Label>
        <Label FontSize="24" FontWeight="Bold" Padding="5">Vikt (gram):</Label>
        <TextBox Name="txbcalc" Padding="5" Margin="5 0 5 10"></TextBox>
        <Button Click="KlickRäkna" Padding="2" Margin="5">Buttooon</Button>
        <Label Name="Resultat"></Label>

    </StackPanel>
</Window>
